{
  "env": {
    "development": {
      "presets": [
        [
          "poi/babel",
          {
            "jsx": "react"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-async-to-generator",
        "transform-class-properties"
      ],
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs"
          }
        ],
        "@babel/preset-react",
        "jest"
      ]
    }
  }
}
